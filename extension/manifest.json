{
    "manifest_version": 2,
    "name": "Edsby Scraper for Project Harkness",
    "description": "Scrapes Edsby for Class Info",
    "version": "1.0",
    "permissions": [
        "declarativeContent"
    ],
    "icons": {
        "16": "images/logo_16.png",
        "32": "images/logo_32.png",
        "48": "images/logo_48.png",
        "128": "images/logo_128.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": true,
        "js":      [ "scripts/jquery.min.js", "content.js" ]
    }],
    "page_action": {
        "default_icon": {
            "16": "images/logo_16.png",
            "32": "images/logo_32.png",
            "48": "images/logo_48.png",
            "128": "images/logo_128.png"
        },
        "default_title": "Edsby Scraper",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}